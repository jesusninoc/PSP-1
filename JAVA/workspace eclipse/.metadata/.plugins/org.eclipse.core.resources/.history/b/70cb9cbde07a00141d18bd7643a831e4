import java.applet.Applet;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;


public class AppletMain extends Applet implements Runnable, ActionListener{

	public void start(){
		RpasoHilos2Applet rH1 = new RpasoHilos2Applet(1,2,1,1);
		RepasoHilos2 rH2 = new RepasoHilos2(10,11,1,1);
		RepasoHilos2 rH3 = new RepasoHilos2(20,30,1,1);
		rH1.start();
		rH2.start();
		rH3.start();
	}
	
	@Override
	public void actionPerformed(ActionEvent arg0) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		
	}

}
